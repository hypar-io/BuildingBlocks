{
  "$schema": "https://hypar.io/Schemas/Function.json",
  "id": "c3bb78fd-7dee-41cc-ae2e-f1b4b4ae2794",
  "name": "Floors By Levels",
  "description": "Creates floors from level perimeters supplied by another function.",
  "language": "C#",
  "model_dependencies": [
    {
      "autohide": true,
      "name": "Levels",
      "optional": false
    },
    {
      "autohide": false,
      "name": "Core",
      "optional": true
    }
  ],
  "model_output": "Floors",
  "input_schema": {
    "type": "object",
    "properties": {
      "Floor Setback": {
        "minimum": 0,
        "maximum": 0.5,
        "multipleOf": 0.1,
        "$hyparUnitType": "length",
        "description": "Setback of all floors from each level's perimeter.",
        "default": 0,
        "type": "number",
        "$hyparOrder": 0
      },
      "Floor Thickness": {
        "minimum": 0.01,
        "maximum": 0.5,
        "multipleOf": 0.01,
        "default": 0.1,
        "$hyparUnitType": "length",
        "description": "Thickness of all floors.",
        "type": "number",
        "$hyparOrder": 1
      }
    }
  },
  "outputs": [
    {
      "unit_type": "area",
      "name": "Total Area",
      "description": "Aggregate area of all floors.",
      "type": "number"
    },
    {
      "unit_type": "none",
      "name": "Floor Quantity",
      "description": "Quantity of floors.",
      "type": "number"
    }
  ],
  "element_types": [
    "https://schemas.hypar.io/LevelPerimeter.json",
    "https://schemas.hypar.io/LevelVolume.json"
  ],
  "repository_url": "https://github.com/hypar-io/BuildingBlocks/tree/master/Floors/FloorsByLevels",
  "source_file_key": null,
  "messages": {
    "ja": {
      "name": "階層別フロア",
      "description": "別の関数で与えられたレベルの周囲からフロアを作成します。",
      "input_schema": {
        "properties": {
          "Floor Setback": {
            "description": "各階の外周からの全フロアのセットバック。",
            "name": "フロアセットバック"
          },
          "Floor Thickness": {
            "description": "すべての床の厚さ",
            "name": "床の厚さ"
          }
        }
      },
      "outputs": [
        {
          "name": "総面積",
          "description": "全フロアの面積を集計。"
        },
        {
          "name": "階数",
          "description": "床の量。"
        }
      ]
    }
  }
}